      h1 {font-size: 34px; line-height: 40px; color: #212322; font-weight: bold; margin: 0 0 15px 0;} h2 {font-size: 30px; line-height: 36px; color: #212322; font-weight: bold; margin: 0 0 15px 0;} h3 {font-size: 26px; line-height: 30px; color: #212322; font-weight: bold;  margin: 0 0 15px 0;} h4 {font-size: 22px; line-height: 25px; color: #212322; font-weight: bold;  margin: 0;} h5 {font-size: 18px; line-height: 25px; color: #212322; font-weight: bold; margin: 0 0 15px 0;} h6 {font-size: 18px; line-height: 25px; color: #212322; font-weight: regular; }     
